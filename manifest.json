{

    "manifest_version": 2,
    "name": "Funreading-extension",
    "version": "1.0",
  
    "description": "extension de FunReading",
    "icons": {
      "32": "icon32.png",
      "48": "icon48.png",
      "96": "icon96.png"
    },
  
    "permissions": [
      "activeTab"
    ],
  
    "browser_action": {
      "default_icon": "icon96.png",
      "default_title": "Funreading",
      "default_popup": "popup.html"
    },
  
    "web_accessible_resources": [

    ]
  
  }
  